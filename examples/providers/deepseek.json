{
  "_info": "DeepSeek V3 Provider-Config fuer OpenClaw",
  "_hinweis": "[MANUELL] Ersetze DEIN-API-KEY-HIER mit deinem echten DeepSeek API Key",
  "_docs": "https://platform.deepseek.com",
  "_getestet": "Noch nicht getestet â€” Template basierend auf OpenClaw-Docs",

  "env": {
    "DEEPSEEK_API_KEY": "DEIN-API-KEY-HIER"
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "deepseek/deepseek-chat"
      }
    }
  },
  "models": {
    "mode": "merge",
    "providers": {
      "deepseek": {
        "baseUrl": "https://api.deepseek.com/v1",
        "apiKey": "${DEEPSEEK_API_KEY}",
        "api": "openai-chat",
        "models": [
          {
            "id": "deepseek-chat",
            "name": "DeepSeek V3",
            "reasoning": false,
            "input": ["text"],
            "cost": { "input": 0.27, "output": 1.1, "cacheRead": 0.07, "cacheWrite": 0 },
            "contextWindow": 64000,
            "maxTokens": 8192
          }
        ]
      }
    }
  }
}
