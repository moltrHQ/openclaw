{
  "_info": "DeepSeek V3 provider config for OpenClaw",
  "_note": "Replace YOUR-API-KEY-HERE with your actual DeepSeek API key",
  "_docs": "https://platform.deepseek.com",
  "_tested": "Not yet tested â€” template based on OpenClaw docs",

  "env": {
    "DEEPSEEK_API_KEY": "YOUR-API-KEY-HERE"
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "deepseek/deepseek-chat"
      }
    }
  },
  "models": {
    "mode": "merge",
    "providers": {
      "deepseek": {
        "baseUrl": "https://api.deepseek.com/v1",
        "apiKey": "${DEEPSEEK_API_KEY}",
        "api": "openai-chat",
        "models": [
          {
            "id": "deepseek-chat",
            "name": "DeepSeek V3",
            "reasoning": false,
            "input": ["text"],
            "cost": { "input": 0.27, "output": 1.1, "cacheRead": 0.07, "cacheWrite": 0 },
            "contextWindow": 64000,
            "maxTokens": 8192
          }
        ]
      }
    }
  }
}
