{
  "_info": "Minimax M2.5 provider config for OpenClaw",
  "_note": "Replace YOUR-API-KEY-HERE with your actual Minimax API key",
  "_docs": "https://www.minimax.io â€” Create account, generate API key",
  "_tested": "2026-02-17, response time 6.7s on native Linux",

  "env": {
    "MINIMAX_API_KEY": "YOUR-API-KEY-HERE"
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "minimax/MiniMax-M2.5"
      }
    }
  },
  "models": {
    "mode": "merge",
    "providers": {
      "minimax": {
        "baseUrl": "https://api.minimax.io/anthropic",
        "apiKey": "${MINIMAX_API_KEY}",
        "api": "anthropic-messages",
        "models": [
          {
            "id": "MiniMax-M2.5",
            "name": "MiniMax M2.5",
            "reasoning": false,
            "input": ["text"],
            "cost": { "input": 15, "output": 60, "cacheRead": 2, "cacheWrite": 10 },
            "contextWindow": 200000,
            "maxTokens": 8192
          }
        ]
      }
    }
  }
}
