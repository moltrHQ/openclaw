{
  "_info": "OpenAI GPT-4o provider config for OpenClaw",
  "_note": "Replace YOUR-API-KEY-HERE with your actual OpenAI API key",
  "_docs": "https://platform.openai.com/api-keys",
  "_tested": "Not yet tested â€” template based on OpenClaw docs",

  "env": {
    "OPENAI_API_KEY": "YOUR-API-KEY-HERE"
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "openai/gpt-4o"
      }
    }
  },
  "models": {
    "mode": "merge",
    "providers": {
      "openai": {
        "baseUrl": "https://api.openai.com/v1",
        "apiKey": "${OPENAI_API_KEY}",
        "api": "openai-chat",
        "models": [
          {
            "id": "gpt-4o",
            "name": "GPT-4o",
            "reasoning": false,
            "input": ["text", "image"],
            "cost": { "input": 5, "output": 15, "cacheRead": 2.5, "cacheWrite": 0 },
            "contextWindow": 128000,
            "maxTokens": 16384
          }
        ]
      }
    }
  }
}
